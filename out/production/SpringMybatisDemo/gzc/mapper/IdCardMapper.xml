<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="gzc.dao.IdCardDao">
    <!--根据id查询身份证号码-->
    <select id="selectCodeById" resultType="gzc.pojo.IdCard" parameterType="Integer">
        select * from idcard where id=#{id}
    </select>
</mapper>